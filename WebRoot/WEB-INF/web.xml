<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LianJieServlet</servlet-name>
    <servlet-class>com.servlet.LianJieServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SuccessServlet</servlet-name>
    <servlet-class>com.servlet.SuccessServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FaliServelt</servlet-name>
    <servlet-class>com.servlet.FaliServelt</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TiJiaoServlet</servlet-name>
    <servlet-class>com.servlet.TiJiaoServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FangWenServlet</servlet-name>
    <servlet-class>com.servlet.FangWenServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>LianJieServlet</servlet-name>
    <url-pattern>/lj</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SuccessServlet</servlet-name>
    <url-pattern>/su</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FaliServelt</servlet-name>
    <url-pattern>/Fali</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TiJiaoServlet</servlet-name>
    <url-pattern>/TiJiao</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FangWenServlet</servlet-name>
    <url-pattern>/FangWen</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
